package controller;

public class Questao03 {

	/*
	 * Desenvolver um programa que apresente os quadrados dos n√∫meros inteiros de 15
	 * a 200.
	 * 
	 * Math.pow(base, expoente)
	 */

	public static void main(String[] args) {
		
		double cont = 15;
		
		while ( cont <= 200 ) {
			System.out.println(cont + " elevado ao quadrado = " + Math.pow(cont, 2));
			cont = cont + 1;
		}
		

	}

}
