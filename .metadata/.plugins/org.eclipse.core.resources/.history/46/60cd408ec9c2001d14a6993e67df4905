package controller2;

public class Questao03b {

	/*
	 * Desenvolver um programa que apresente o total da soma de cinco valores informados pelo usu√°rio
	 */

	public static void main(String[] args) {
		
		int cont = 1;
		
		do {
			System.out.println( cont );
			
			cont ++;
		} while ( cont <= 5 );
		

	}

}
