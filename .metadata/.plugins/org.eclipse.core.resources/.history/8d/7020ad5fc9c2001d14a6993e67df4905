package controller2;

public class Questao03b {

	/*
	 * Desenvolver um programa que apresente o total da soma dos cinco primeiros
	 * números inteiros.
	 */

	public static void main(String[] args) {
		int cont = 1, acum = 0;
		
		do {
			acum = acum + cont;
			cont ++;
		} while ( cont <= 5 );
		
		System.out.println("A soma dos cinco primeiros números é " + acum);
		

	}

}
