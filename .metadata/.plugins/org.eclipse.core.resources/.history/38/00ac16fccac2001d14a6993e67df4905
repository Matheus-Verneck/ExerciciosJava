package controller2;

import util.Teclado;

public class Questao03b {

	/*
	 * Desenvolver um programa que apresente o total da soma de cinco valores informados pelo usuário
	 */

	public static void main(String[] args) {
		
		int cont = 1, num;
		
		do {
			num = Teclado.lerInt("Informe um número: ");
			
			cont ++;
		} while ( cont <= 5 );
		

	}

}
