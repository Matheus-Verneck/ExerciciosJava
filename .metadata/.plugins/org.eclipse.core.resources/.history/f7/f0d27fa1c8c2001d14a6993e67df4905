package controller2;

public class Questao03 {

	/*
	 * Desenvolver um programa que apresente o total da soma dos cinco primeiros
	 * n√∫meros inteiros.
	 */

	public static void main(String[] args) {
		int cont = 1, acum = 0;
		
		do {
			System.out.println( cont );
			cont ++;
		} while ( cont <= 5 );
		

	}

}
